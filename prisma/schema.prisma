// prisma/schema.prisma

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Product {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  price       Float
  category    String
}